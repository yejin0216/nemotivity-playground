{
  "env": {
    "browser": true,
    "es2020": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:jsdoc/recommended",
    "plugin:markdown/recommended"
  ],
  "ignorePatterns": [
    "setupJest.js",
    "node_modules/*",
    "dist/*",
    "test/*"
  ],
  "rules": {
    "indent": [
      "error",
      2
    ],
    "quotes": [
      "error",
      "single"
    ]
  }
}